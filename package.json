{
  "name": "el-camino",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pwa": "npx pwa-asset-generator public/el-camino_logo.png .public/pwa-assets --manifest src/manifest.webmanifest --index src/index.html --type png --maskable true --opaque false --mstile --favicon",
    "pwa:assets-icons": "npx pwa-asset-generator public/el-camino_logo.png public/pwa-assets --only-icons --index src/index.html --favicon",
    "doc:g": "npx @compodoc/compodoc -p ./tsconfig.json --output ./docs --name \"Sistema RDS\" --customFavicon ./src/assets/icons/favicon-196.png --customLogo ./src/assets/images/rds-newlogo-transparent.png --theme material --hideGenerator --language es-ES",
    "deploy": "ng deploy",
    "clean": "rm -rf ./node_modules yarn.lock package-lock.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.9",
    "@angular/cdk": "^19.2.14",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.14",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/service-worker": "^19.2.0",
    "@tailwindcss/postcss": "^4.1.5",
    "ngx-spinner": "^19.0.0",
    "primeicons": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "tailwindcss": "^4.1.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.7",
    "@angular/cli": "^19.2.7",
    "@angular/compiler-cli": "^19.2.0",
    "@compodoc/compodoc": "^1.1.26",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.3",
    "pwa-asset-generator": "^8.0.4",
    "typescript": "~5.7.2"
  }
}
